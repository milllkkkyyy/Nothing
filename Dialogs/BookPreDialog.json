        "_index": 0
    },
    
    {
        "type": "dialogue",
        "speaker_id": "Book",
        "portrait_id": "Normal",
        "text": "It feels horrible to say but I have never felt free like how I felt when I was grounded in North Korea",
        "bg_color": "ffffff",
