[


    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Normal",
        "text": "Hey...my name is Ben.",
        "bg_color":"dcb68c",
        "next": 1,
        "text_speed": 0.03,
        "_index": 0
    },

    {
        "type": "question",
        "speaker_id": "Ben",
        "portrait_id": "Normal",
        "text": "Are you anxious about the news?",
        "bg_color":"dcb68c",
        "questions": {
            "No.": 3,
            "Yeah.": 2
        },
        "text_speed": 0.03,
        "_index": 1
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Happy",
        "text": "Thank god, someone who knows what is going on.",
        "bg_color":"dcb68c",
        "next": 15,
        "text_speed": 0.03,
        "_index": 2
    },

    {
        "type": "question",
        "speaker_id": "Ben",
        "portrait_id": "Sad",
        "text": "Really?",
        "bg_color":"dcb68c",
        "questions": {
            "Why should I?": 9,
            "I'm chilling.": 4
        },
        "text_speed": 0.03,
        "_index": 3
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Sad",
        "text": "There is so much to be fucking anxious about!",
        "bg_color":"dcb68c",
        "next": 5,
        "text_speed": 0.03,
        "_index": 4
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Sad",
        "text": "There is a FUCKING PANDEMIC that could kill billions of people!",
        "bg_color":"dcb68c",
        "next": 6,
        "text_speed": 0.03,
        "_index": 5
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Sad",
        "text": "HAPPENING RIGHT FUCKING NOW!",
        "bg_color":"dcb68c",
        "next": 7,
        "text_speed": 0.03,
        "_index": 6
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Sad",
        "text": "WHY ARE WE ALL IGNORING IT?",
        "bg_color":"dcb68c",
        "next": 8,
        "text_speed": 0.03,
        "_index": 7
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Sad",
        "text": "[Everyone is looking at you. Ben notices. He says sorry and takes off]",
        "bg_color":"dcb68c",
        "next": 34,
        "text_speed": 0.03,
        "_index": 8
    },

    {
        "type": "question",
        "speaker_id": "Ben",
        "portrait_id": "Normal",
        "text": "Have you not heard the news?",
        "bg_color":"dcb68c",
        "questions": {
            "Yes.": 2,
            "No.": 10
        },
        "text_speed": 0.03,
        "_index": 9
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Normal",
        "text": "What?",
        "bg_color":"dcb68c",
        "next": 11,
        "text_speed": 0.03,
        "_index": 10
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Sad",
        "text": "You are choicefully ignorant huh?",
        "bg_color":"dcb68c",
        "next": 12,
        "text_speed": 0.03,
        "_index": 11
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Normal",
        "text": "There is a fucking pandemic in Europe right now.",
        "bg_color":"dcb68c",
        "next": 13,
        "text_speed": 0.03,
        "_index": 12
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Normal",
        "text": "People think its gonna be the new plague.",
        "bg_color":"dcb68c",
        "next": 14,
        "text_speed": 0.03,
        "_index": 13
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Sad",
        "text": "God though, I feel so fucking bad.",
        "bg_color":"dcb68c",
        "next": 15,
        "text_speed": 0.03,
        "_index": 14
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Normal",
        "text": "I feel like nobody is caring about this shit.",
        "bg_color":"dcb68c",
        "next": 16,
        "text_speed": 0.03,
        "_index": 15
    },
    
    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Normal",
        "text": "I care but it feels like its for the wrong reason.",
        "bg_color":"dcb68c",
        "next": 17,
        "text_speed": 0.03,
        "_index": 16
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Normal",
        "text": "Im a financial guy. I mess around with stocks.",
        "bg_color":"dcb68c",
        "next": 18,
        "text_speed": 0.03,
        "_index": 17
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Happy",
        "text": "I'm gonna make it big when this shit hits the United States man.",
        "bg_color":"dcb68c",
        "next": 19,
        "text_speed": 0.03,
        "_index": 18
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Normal",
        "text": "I am just so anxious though.",
        "bg_color":"dcb68c",
        "next": 20,
        "text_speed": 0.03,
        "_index": 19
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Sad",
        "text": "What if I die? What if my parents die?",
        "bg_color":"dcb68c",
        "next": 21,
        "text_speed": 0.03,
        "_index": 20
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Normal",
        "text": "Like is the money even gonna be worth it then?",
        "bg_color":"dcb68c",
        "next": 22,
        "text_speed": 0.03,
        "_index": 21
    },

    {
        "type": "dialogue",
        "speaker_id": "Ben",
        "portrait_id": "Normal",
        "text": "Fuck man, I am sorry for being depressing. This shit is just scary.",
        "bg_color":"dcb68c",
        "next": 34,
        "text_speed": 0.03,
        "_index": 22
    }


]